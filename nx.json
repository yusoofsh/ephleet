{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"npmScope": "ephleet",
	"affected": {
		"defaultBase": "main"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/nx-cloud",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e", "check"],
				"accessToken": "ZjZhZTY2MjAtMDc1MC00NmEzLWFiMDEtNDJmM2UzZmUzNjMwfHJlYWQtd3JpdGU="
			}
		}
	},
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"]
		}
	},
	"generators": {
		"@nrwl/react": {
			"application": {
				"babel": true
			}
		},
		"@nrwl/next": {
			"application": {
				"style": "css",
				"linter": "eslint"
			}
		}
	},
	"defaultProject": "web",
	"plugins": ["@nxtensions/astro"]
}
